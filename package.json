{
  "name": "umi-web-template",
  "version": "1.1.0",
  "private": true,
  "description": "A web template",
  "scripts": {
    "build": "max build",
    "build:analyze": "cross-env ANALYZE=1 max build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "max setup && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "release": "semantic-release",
    "start": "max dev",
    "start:no-mock": "cross-env MOCK=none max dev",
    "start:no-ui": "cross-env UMI_UI=none max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none max dev",
    "test": "max test",
    "test:component": "max test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@ant-design/pro-components": "1.1.23",
    "@umijs/max": "4.0.22",
    "@umijs/route-utils": "2.2.0",
    "antd": "4.23.2",
    "classnames": "2.3.2",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "omit.js": "2.0.2",
    "rc-menu": "9.6.4",
    "rc-util": "5.24.4",
    "react": "17.0.2",
    "react-dev-inspector": "1.8.1",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.3.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "2.1.5",
    "@playwright/test": "1.26.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/classnames": "2.3.0",
    "@types/express": "4.17.14",
    "@types/history": "4.7.11",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.185",
    "@types/qs": "6.9.7",
    "@types/react": "17.0.50",
    "@types/react-dom": "17.0.17",
    "@types/react-helmet": "6.1.5",
    "@umijs/fabric": "2.14.0",
    "@umijs/max": "4.0.22",
    "@umijs/openapi": "1.7.0",
    "@umijs/yorkie": "2.0.5",
    "@vercel/node": "1.15.4",
    "chalk": "4.1.2",
    "commitlint": "17.1.2",
    "commitlint-config-gitmoji": "2.2.5",
    "conventional-changelog-gitmoji-config": "1.4.3",
    "cross-env": "7.0.3",
    "cross-port-killer": "1.4.0",
    "detect-installer": "1.0.2",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.5.4",
    "mockjs": "1.1.0",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "semantic-release-config-gitmoji": "1.2.4",
    "stylelint": "13.13.1",
    "swagger-ui-dist": "4.14.1",
    "typescript": "4.8.3",
    "umi-presets-pro": "1.0.5",
    "umi-serve": "1.9.11"
  }
}
