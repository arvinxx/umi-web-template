{
  "name": "umi-web-template",
  "version": "1.1.0",
  "private": true,
  "description": "A web template",
  "scripts": {
    "build": "max build",
    "build:analyze": "cross-env ANALYZE=1 max build",
    "dev": "npm run start:dev",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "max setup && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "release": "semantic-release",
    "start": "max dev",
    "start:no-mock": "cross-env MOCK=none max dev",
    "start:no-ui": "cross-env UMI_UI=none max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none max dev",
    "test": "max test",
    "test:component": "max test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^1.1.8",
    "@umijs/max": "^4.0.6",
    "@umijs/route-utils": "^2.1.4",
    "antd": "^4.20.0",
    "classnames": "^2.3.0",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "omit.js": "^2.0.2",
    "rc-menu": "^9.1.0",
    "rc-util": "^5.16.0",
    "react": "^17.0.0",
    "react-dev-inspector": "^1.7.0",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.2.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.4",
    "@playwright/test": "^1.17.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.11.1",
    "@umijs/max": "^4.0.0-rc.22",
    "@umijs/openapi": "^1.6.0",
    "@umijs/yorkie": "^2.0.3",
    "@vercel/node": "^1.9.0",
    "chalk": "^4.0.0",
    "commitlint": "^17.0.3",
    "commitlint-config-gitmoji": "^2.2.1",
    "conventional-changelog-gitmoji-config": "^1.4.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "jsdom-global": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "semantic-release-config-gitmoji": "^1.2.4",
    "stylelint": "^13.0.0",
    "swagger-ui-dist": "^4.12.0",
    "typescript": "^4.7.4",
    "umi-presets-pro": "^1.0.1",
    "umi-serve": "^1.9.10"
  }
}
